local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()

local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "Raxoner"})

OrionLib:MakeNotification({
	Name = "Raxoner Hub Is Loading!",
	Content = "Khusus warga raxon",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local RaxTab = Window:MakeTab({
	Name = "Raxoner Hub",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = RaxTab:AddSection({
	Name = "Raxoner"
})

RaxTab:AddButton({
	Name = "Pilih woi!",
	Callback = function()
      		print("Pencet")
  	end    
})

RaxTab:AddToggle({
	Name = "Toggle!",
	Default = false,
	Callback = function(Value)
		print("Pencet")
	end    
})

RaxTab:AddColorpicker({
	Name = "Colorpicker",
	Default = Color3.fromRGB(255, 0, 0),
	Callback = function(Value)
		print(Value)
	end	  
})

RaxTab:AddSlider({
	Name = "Slider",
	Min = 0,
	Max = 20,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "bananas",
	Callback = function(Value)
		print(Value)
	end    
})

OrionLib:Init()
